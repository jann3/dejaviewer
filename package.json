{
  "name": "dejaviewer",
  "version": "0.1.14",
  "description": "Image viewer with hot loading",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "(if exist \"build/win\" rmdir  /s /q \"build/win\") && electron-packager ./ --platform=win32 --arch=x64 --out=build --icon=./build/assets/dejaviewer-icon-square-small.ico && move build/DejaViewer-win32-x64 build/win",
    "package": "node src/package.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jann3/DejaViewer.git"
  },
  "keywords": [
    "deja",
    "viewer",
    "photo",
    "image",
    "gallery"
  ],
  "author": "Jann3",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^29.0.1",
    "electron-packager": "^17.1.2",
    "electron-winstaller": "^5.2.1"
  },
  "dependencies": {
    "image-size": "^1.1.1",
    "util.promisify": "^1.1.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "authors": "Jann3",
            "description": "Image viewer with hot loading"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}